{
  "expo": {
    "name": "controll",
    "slug": "controll",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "utils/assets/images/icon.png",
    "scheme": "controllnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lizandromelo.controll"
    },
    "web": {
      "output": "static",
      "favicon": "./utils/assets/images/logo_192x192.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./utils/assets/images/logo_192x192.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "97c5dbee-38d5-4d20-8b26-9e260d920996"
      }
    }
  }
}